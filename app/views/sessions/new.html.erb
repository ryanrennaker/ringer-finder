<form action="/authenticate" class="col-md-4">
  <h1>Sign in</h1>
  <div class="form-group">
    <input name="fname" type="text" class="form-control input-md" placeholder="Username">
  </div>
  <div class="form-group">
    <input name="pwd" type="text" class="form-control input-md" placeholder="Password">
  </div>
  <button type="submit" class="btn btn-success btn-md">Sign in</button>
</form>
